complete <- function(directory,id = 1:332){
x <- directory
y <-paste("~",x,sep = "/")
setwd(y)
df <-data.frame()
  for (i in id){
    v <-read.csv(paste0(formatC(i,width = 3 ,format = "d",flag = "0"),".csv"))
    y <-sum(complete.cases(v))
    df1 <-data.frame(id = i ,nobs = y )
    final_df <- rbind(df,df1)
    print(final_df)
  }
######notice : if put print here , the result is incorrect
}
